{
  "info": {
    "name": "Cloud Computing Project v1 - Final",
    "description": "API Collection sesuai hasil testing Postman (Google Apps Script)",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "BASE_URL",
      "value": "https://script.google.com/macros/s/AKfycbyHS84TjZWi1S1d-qK6SHawbYFIQENIrKh7mG5TbZbJs-hgebyqvrYXhvjn6E7QPqSY7A/exec"
    }
  ],
  "item": [
    {
      "name": "1. presence/qr_generate",
      "request": {
        "method": "POST",
        "header": [
          { "key": "Content-Type", "value": "application/json" }
        ],
        "url": {
          "raw": "{{BASE_URL}}?action=presence/qr_generate",
          "host": ["{{BASE_URL}}"],
          "query": [
            { "key": "action", "value": "presence/qr_generate" }
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"course_id\": \"cloud-101\",\n  \"session_id\": \"sesi-01\",\n  \"ts\": \"{{$isoTimestamp}}\"\n}"
        }
      }
    },
    {
      "name": "2. presence/checkin",
      "request": {
        "method": "POST",
        "header": [
          { "key": "Content-Type", "value": "application/json" }
        ],
        "url": {
          "raw": "{{BASE_URL}}?action=presence/checkin",
          "host": ["{{BASE_URL}}"],
          "query": [
            { "key": "action", "value": "presence/checkin" }
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"user_id\": \"user-001\",\n  \"device_id\": \"dev-001\",\n  \"course_id\": \"cloud-101\",\n  \"session_id\": \"sesi-01\",\n  \"qr_token\": \"TKN-XXXXXX\",\n  \"ts\": \"{{$isoTimestamp}}\"\n}"
        }
      }
    },
    {
      "name": "3. presence/status",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{BASE_URL}}?action=presence/status&user_id=user-001&course_id=cloud-101&session_id=sesi-01",
          "host": ["{{BASE_URL}}"],
          "query": [
            { "key": "action", "value": "presence/status" },
            { "key": "user_id", "value": "user-001" },
            { "key": "course_id", "value": "cloud-101" },
            { "key": "session_id", "value": "sesi-01" }
          ]
        }
      }
    },
    {
      "name": "4. telemetry/gps",
      "request": {
        "method": "POST",
        "header": [
          { "key": "Content-Type", "value": "application/json" }
        ],
        "url": {
          "raw": "{{BASE_URL}}?action=telemetry/gps",
          "host": ["{{BASE_URL}}"],
          "query": [
            { "key": "action", "value": "telemetry/gps" }
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"device_id\": \"dev-001\",\n  \"ts\": \"{{$isoTimestamp}}\",\n  \"lat\": -7.2575,\n  \"lng\": 112.7521,\n  \"accuracy_m\": 10\n}"
        }
      }
    },
    {
      "name": "5. telemetry/gps/latest",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{BASE_URL}}?action=telemetry/gps/latest&device_id=dev-001",
          "host": ["{{BASE_URL}}"],
          "query": [
            { "key": "action", "value": "telemetry/gps/latest" },
            { "key": "device_id", "value": "dev-001" }
          ]
        }
      }
    },
    {
      "name": "6. telemetry/accel",
      "request": {
        "method": "POST",
        "header": [
          { "key": "Content-Type", "value": "application/json" }
        ],
        "url": {
          "raw": "{{BASE_URL}}?action=telemetry/accel",
          "host": ["{{BASE_URL}}"],
          "query": [
            { "key": "action", "value": "telemetry/accel" }
          ]
        },
        "body": {
          "mode": "raw",
          "raw": "{\n  \"device_id\": \"dev-001\",\n  \"ts\": \"{{$isoTimestamp}}\",\n  \"samples\": [\n    {\n      \"t\": \"{{$isoTimestamp}}\",\n      \"x\": 0.12,\n      \"y\": 0.01,\n      \"z\": 9.81\n    }\n  ]\n}"
        }
      }
    },
    {
      "name": "7. telemetry/accel/latest",
      "request": {
        "method": "GET",
        "url": {
          "raw": "{{BASE_URL}}?action=telemetry/accel/latest&device_id=dev-001",
          "host": ["{{BASE_URL}}"],
          "query": [
            { "key": "action", "value": "telemetry/accel/latest" },
            { "key": "device_id", "value": "dev-001" }
          ]
        }
      }
    }
  ]
}